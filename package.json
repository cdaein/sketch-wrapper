    "build": "tsc --noemit && tsup",
    "build-vite": "vite build --config vite.build.config.js",
    "demo": "vite ./demo --config vite.demo.config.js"
  },
  "author": "Daeinc",
  "license": "MIT",
  "dependencies": {
    "@daeinc/canvas": "^0.13.0",
    "@daeinc/dom": "^0.4.1",
    "gifenc": "^1.0.3",
    "webm-muxer": "^1.1.5"
  },
  "devDependencies": {
    "@daeinc/color": "^0.2.4",
    "@daeinc/draw": "^0.3.0",
    "@daeinc/math": "^0.5.0",
    "@types/node": "^18.11.18",
    "@types/p5": "^1.4.3",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "license-checker": "^25.0.1",
    "p5": "^1.5.0",
    "parcel": "^2.8.3",
    "pts": "^0.11.3",
    "rollup-plugin-dts": "^5.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-typescript": "^1.0.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
