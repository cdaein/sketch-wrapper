  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdaein/sketch-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/cdaein/sketch-wrapper/issues"
  },
  "homepage": "https://github.com/cdaein/sketch-wrapper#readme"
