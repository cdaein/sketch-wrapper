{
  "name": "@daeinc/sketch-wrapper",
  "version": "0.13.1",
  "description": "Helpers for creative coding sketches with HTML Canvas",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "test": "jest --watch",
    "prebuild": "license-checker --exclude 'MIT, MIT OR X11, BSD, ISC' -production",
